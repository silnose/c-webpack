{
  "name": "c-webpack",
  "version": "1.0.0",
  "description": "Webpack es un empaquetador para Javascript y sus amigos. Convierte módulos con dependencias en archivos estáticos que los navegadores entienden.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:first": "webpack --entry ./index.js --output ./bundle.js --mode production",
    "build": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-point/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev-server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:hot-module": "webpack-dev-server --config ./hot-module-replacement/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silnose/c-webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@silnose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silnose/c-webpack/issues"
  },
  "homepage": "https://github.com/silnose/c-webpack#readme",
  "dependencies": {},
  "devDependencies": {
    "css-loader": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "style-loader": "0.23.1",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.5.1"
  }
}
